msgid "Boot continues in 10 sec"
msgstr ""

msgid "Enter LUKS passphrase"
msgstr "បញ្ចូល​ឃ្លា​សម្ងាត់ LUKS"

#
msgid "Do you accept the license agreement ?"
msgstr "តើ​អ្នក​ព្រម​ទទួល​កិច្ចព្រមព្រៀង​អាជ្ញាប័ណ្ណ​ដែរឬទេ ?"

# power-off message
msgid "Do you want to start the System-Recovery ?"
msgstr "តើ​អ្នក​ចង់​ចាប់ផ្ដើម​ការ​សង្គ្រោះ​ប្រព័ន្ធ​ដែរ​ឬទេ ?"

# power-off message
msgid "Do you want to start the System-Restore ?"
msgstr "តើ​អ្នក​ចង់​ចាប់ផ្ដើម​ការ​ស្ដារ​ប្រព័ន្ធ​ដែរឬទេ ?"

#
msgid "Restoring base operating system..."
msgstr "កំពុង​ស្ដារ​ប្រព័ន្ធ​ប្រតិបត្តិការ​មូលដ្ឋាន..."

msgid "Destroying ALL data on %1, continue ?"
msgstr "បំផ្លាញ​ទិន្នន័យ​ទាំង​អស់​នៅ​លើ %1 បន្ត ?"

msgid "Clean sweep..."
msgstr "បោស​សម្អាត...​"

msgid "%1 files from %2 restored..."
msgstr "បាន​ស្ដារ​ឯកសារ %1 ពី %2 ..."

msgid "backing up: %1..."
msgstr "កំពុង​បម្រុងទុក ៖ %1..."

#
msgid "archiving: %1..."
msgstr "កំពុង​ទុក​ក្នុង​ប័ណ្ណសារ ៖ %1..."

#
msgid "restoring: %1..."
msgstr "កំពុង​ស្ដារ ៖ %1..."

msgid "Yes"
msgstr "បាទ/ចាស"

msgid "No"
msgstr "ទេ"

# ok button label
msgid "OK"
msgstr "យល់ព្រម"

# cancel button label
msgid "Cancel"
msgstr "បោះបង់"

msgid "Exit"
msgstr "ចេញ"

msgid "Bootloader installation has failed"
msgstr ""

msgid ""
"The system will not be able to reboot. Please make sure to fixup and install "
"the bootloader before next reboot. Check /var/log/boot.kiwi for details"
msgstr ""

#
msgid "Please remove the CD/DVD before reboot"
msgstr "សូម​យក​ស៊ីឌី/ឌីវីឌី​ចេញ​មុន​នឹង​ចាប់ផ្ដើម​ឡើង​វិញ"

#
msgid "Please unplug the USB stick before reboot"
msgstr "សូម​ដក​ឧបករណ៍​ផ្ទុក​ចល័ត (យូអេសប៊ី) ចេញ​មុន​នឹង​ចាប់ផ្ដើម​ឡើងវិញ"

#
msgid "Installation..."
msgstr "ការ​ដំឡើង..."

# progress indicator label
msgid "Loading %1"
msgstr "កំពុង​ផ្ទុក %1"

msgid "Verifying %1"
msgstr "កំពុង​ផ្ទៀងផ្ទាត់ %1"

msgid "Select disk for installation:"
msgstr "ជ្រើស​ថាស​សម្រាប់​ដំឡើង ៖"

#
msgid "Select %1 partition for installation:"
msgstr "ជ្រើស​ភាគ​ថាស %1 សម្រាប់​ដំឡើង ៖"

msgid "System will be shutdown. Remove USB stick before power on"
msgstr ""

#
#, fuzzy
msgid "Please remove the CD/DVD before shutdown"
msgstr "សូម​យក​ស៊ីឌី/ឌីវីឌី​ចេញ​មុន​នឹង​ចាប់ផ្ដើម​ឡើង​វិញ"
